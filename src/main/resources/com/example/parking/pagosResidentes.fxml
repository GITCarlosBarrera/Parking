<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.parking.PagosController"
      styleClass="contenedor">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Nombre del informe:" styleClass="label"/>

    <TextField fx:id="nombreInforme" styleClass="campo-texto"/>

    <Button text="Crear informe" onAction="#crearInforme" styleClass="boton-menu"/>
</VBox>
